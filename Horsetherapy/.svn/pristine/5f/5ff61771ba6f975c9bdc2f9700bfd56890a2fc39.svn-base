package sepm.ss13.e0828.domain;

public class Therapy {

	private int therapyId =0;
	private float therapyHours =0.0f;
	private float therapyPrice =0.0f;
	private boolean therapyTaken = false;
	private String therapyPerson ="";
	private Horse therapyHorse = null;
	private Invoice invoice = null;
	
	/* Constructor comes */
	
	
	public int getTherapyId() {
		return therapyId;
	}
	public void setTherapyId(int therapyId) {
		this.therapyId = therapyId;
	}
	public float getTherapyHours() {
		return therapyHours;
	}
	public void setTherapyHours(float therapyHours) {
		this.therapyHours = therapyHours;
	}
	public float getTherapyPrice() {
		return therapyPrice;
	}
	public void setTherapyPrice(float therapyPrice) {
		this.therapyPrice = therapyPrice;
	}
	public boolean isTherapyTaken() {
		return therapyTaken;
	}
	public void setTherapyTaken(boolean therapyTaken) {
		this.therapyTaken = therapyTaken;
	}
	public String getTherapyPerson() {
		return therapyPerson;
	}
	public void setTherapyPerson(String therapyPerson) {
		this.therapyPerson = therapyPerson;
	}
	public Horse getTherapyHorse() {
		return therapyHorse;
	}
	public void setTherapyHorse(Horse therapyHorse) {
		this.therapyHorse = therapyHorse;
	}
	public Invoice getInvoice() {
		return invoice;
	}
	public void setInvoice(Invoice invoice) {
		this.invoice = invoice;
	}
	
}
